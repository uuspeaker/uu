.note-splash {
    position: fixed;
    bottom: 100rpx;
    left: 50%;
    margin-left: -250rpx;
    z-index: 900;
    width: 500rpx;
    height: 312rpx;
}
.bottom{
  position: fixed;
  bottom: 5px;
  width:50%;
  margin-left: 23% 
}
.note-btn {
    position: fixed;
    bottom: 40rpx;
    left: 50%;
    margin-left: -70rpx;
    width: 140rpx;
    height: 140rpx;
    background: rgb(136, 186, 71);
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0 2px 10px rgba(0, 49, 114, .5);
    transition: all 300ms ease;
    z-index: 1000;
}
.note-gradient {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 220rpx;
    background: linear-gradient(to top, rgba(255,255,255,1) 30%, rgba(255,255,255,0));
    z-index: 900;
}
.date {
  padding-top: 5px;
  font-size: 13px;
  color: grey;
  padding-left: 10px; 
}